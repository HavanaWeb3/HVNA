# Environment Variables for Token Pre-Sale Deployment

# Contract Addresses (update with your deployed addresses)
HVNA_TOKEN_ADDRESS=
GENESIS_NFT_ADDRESS=

# Pre-sale Phase Timing (Unix timestamps)
# Genesis Phase Start (default: 24 hours from now)
GENESIS_PHASE_START=
# Genesis Phase End (default: 7 days from start)
GENESIS_PHASE_END=
# Public Phase Start (default: 1 hour after Genesis ends)
PUBLIC_PHASE_START=
# Public Phase End (default: 30 days total pre-sale)
PUBLIC_PHASE_END=

# Network Configuration
INFURA_PROJECT_ID=
PRIVATE_KEY=
ETHERSCAN_API_KEY=

# Example timestamps (remove the # to use):
# GENESIS_PHASE_START=1735689600  # Jan 1, 2025 00:00:00 UTC
# GENESIS_PHASE_END=1736294400    # Jan 8, 2025 00:00:00 UTC
# PUBLIC_PHASE_START=1736298000   # Jan 8, 2025 01:00:00 UTC
# PUBLIC_PHASE_END=1738281600     # Jan 31, 2025 00:00:00 UTC